{
  "name": "nextjs-sandbox",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "prettier": "prettier"
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages",
      "sections"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.7",
    "@webiny/app-website-builder": "0.0.0",
    "autoprefixer": "^10.4.21",
    "next": "^15.1.2",
    "next-seo": "^5.4.0",
    "postcss": "^8.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^4.1.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.1",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "bunyan": "^1.8.14",
    "bunyan-prettystream": "^0.1.3",
    "next-unused": "^0.0.3"
  },
  "license": "MIT"
}
